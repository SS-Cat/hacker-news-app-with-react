[32m+[m[32mconst DEFAULT_HPP = '10'[m
[32m+[m
[32m+[m[32mconst PATH_BASE = 'https://hn.algolia.com/api/v1'[m
[32m+[m[32mconst PATH_SEARCH = '/search'[m
[32m+[m[32mconst PARAM_SEARCH = 'query='[m
[32m+[m[32mconst PARAM_PAGE = 'page='[m
[32m+[m[32mconst PARAM_HPP = 'hitsPerPage='[m
[32m+[m
[32m+[m[32mconst SORTS = {[m
[32m+[m	[32mNONE: list => list,[m
[32m+[m	[32mTITLE: list => R.sortBy(R.compose(R.toLower, R.prop('title')))(list),[m
[32m+[m	[32mAUTHOR: list => R.sortBy(R.compose(R.toLower, R.prop('author')))(list),[m
[32m+[m	[32mCOMMENTS: list => R.sortBy(R.compose(R.prop('num_comments')))(list).reverse(),[m
[32m+[m	[32mPOINTS: list => R.sortBy(R.compose(R.prop('points')))(list).reverse()[m
[32m+[m[32m}[m
