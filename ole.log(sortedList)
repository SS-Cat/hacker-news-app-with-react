[32m+[m[32m  setSearchTopStories = result => {[m
[32m+[m[32m    const { hits, page } = result[m
[32m+[m[32m    const { searchKey, results } = this.state[m
[32m+[m
[32m+[m[32m    const oldHits = results && results[searchKey][m
[32m+[m[32m      ? results[searchKey].hits[m
[32m+[m[32m      : [][m
[32m+[m
[32m+[m[32m    const updateHits = [[m
[32m+[m[32m      ...oldHits,[m
[32m+[m[32m      ...hits[m
[32m+[m[32m    ][m
[32m+[m
[32m+[m[32m    this.setState({[m
[32m+[m[32m      results: {[m
[32m+[m[32m        ...results,[m
[32m+[m[32m        [searchKey]: { hits: updateHits, page }[m
[32m+[m[32m      },[m
[32m+[m[32m      isLoading: false[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  fetchSearchTopStories = (searchTerm, page = 0) => {[m
[32m+[m[32m    this.setState({ isLoading: true })[m
[32m+[m[32m    axios(PATH_BASE + PATH_SEARCH + '?' + PARAM_SEARCH + searchTerm + '&' + PARAM_PAGE + page + '&' + PARAM_HPP + DEFAULT_HPP )[m
[32m+[m[32m      .then(result => this.setSearchTopStories(result.data))[m
[32m+[m[32m      .catch(error => this._isMounted && this.setState({error}))[m
[32m+[m[32m  }[m
