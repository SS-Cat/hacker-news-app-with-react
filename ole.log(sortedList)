[32m+[m[32mconst Sort = ({ sortKey, onSort, children, activeSortKey }) => {[m
[32m+[m	[32mconst sortClass = ['button-inline'][m
[32m+[m	[32mif(sortKey === activeSortKey){[m
[32m+[m		[32msortClass.push('button-active')[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn ([m
[32m+[m		[32m<Button[m[41m [m
[32m+[m			[32monClick={() => onSort(sortKey)}[m[41m [m
[32m+[m			[32mclassName={sortClass.join(' ')}[m
[32m+[m		[32m>[m
[32m+[m			[32m{children}[m
[32m+[m		[32m</Button>[m
[32m+[m	[32m)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mconst Button = ({ onClick, className='', children }) =>[m
[32m+[m[32m  <button[m
